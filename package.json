{
  "name": "playground-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run graphql-codegen:client -- --watch\" \"npm run graphql-codegen:server -- --watch\" \"npm run tailwind\"",
    "tailwind": "concurrently \"next dev\" \"tailwindcss --input ./app/styles/tailwind.css --output ./app/output.css --watch\"",
    "build": "tailwindcss --input ./app/styles/tailwind.css --output ./app/output.css && next build",
    "start": "next start",
    "lint": "next lint",
    "graphql-codegen": "npm run graphql-codegen:server && npm run graphql-codegen:client",
    "graphql-codegen:client": "graphql-codegen --config graphql.config.js --project app",
    "graphql-codegen:server": "graphql-codegen --config graphql.config.js --project api"
  },
  "dependencies": {
    "@apollo/server": "^4.2.2",
    "@as-integrations/next": "^1.2.0",
    "@graphql-tools/mock": "^8.7.14",
    "@headlessui/react": "^1.7.5",
    "@heroicons/react": "^2.0.13",
    "@reduxjs/toolkit": "^1.9.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "clsx": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "focus-visible": "^5.2.0",
    "graphql-tag": "^2.12.6",
    "next": "13.0.6",
    "node-fetch": "^3.3.0",
    "postcss-focus-visible": "^7.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-resolvers": "2.7.10",
    "@graphql-codegen/typescript-rtk-query": "^2.3.7",
    "@graphql-tools/schema": "^9.0.12",
    "@rtk-query/graphql-request-base-query": "^2.2.0",
    "@types/node": "^18.11.13",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "encoding": "^0.1.13",
    "intercept-stdout": "^0.1.2",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
